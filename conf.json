{
  "source": {
    "include": [
      "./package.json",
      "./README.md",
      "./lib"
    ],
    "exclude": [
      "./app",
      "./coverage",
      "./dist",
      "./docs",
      "./gulp",
      "./test"
    ],
    "includePattern": ".+\\.(js|md)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "opts": {
    "template": "",
    "encoding": "utf8",
    "destination": "./docs/dist",
    "recurse": true,
    "tutorials": "./docs/src/tutorials"
  },
  "templates": {
    "default": {
      "includeDate": false,
      "layoutFile": "./docs/src/layout.tmpl",
      "staticFiles": {
        "excludePattern": ".*webfont",
        "includePattern": ".*\\.css$",
        "include": [
          "./docs/src/static"
        ]
      }
    },
    "applicationName": "Anny",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    },
    "linenums": true
  }
}
