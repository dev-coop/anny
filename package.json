{
  "name": "anny",
  "version": "0.1.0",
  "description": "An artificial neural network, yo!",
  "main": "index.js",
  "scripts": {
    "coverage-check": "istanbul check-coverage",
    "coverage-report": "codeclimate-test-reporter < coverage/lcov.info",
    "start": "gulp",
    "test": "babel-node $(npm bin)/isparta cover _mocha -- -R dot",
    "lint": "eslint .",
    "test-watch": "babel-node $(npm bin)/isparta cover _mocha -- -w",
    "postinstall": "bower install",
    "clean": "while read line; do rm -rf $line; done < .gitignore",
    "clean-wercker": "rm -rf _builds _projects _steps"
  },
  "precommit": {
    "silent": false,
    "run": "lint, test, coverage-check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-coop/anny.git"
  },
  "author": "Levi Thomason <me@levithomason.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dev-coop/anny/issues"
  },
  "homepage": "https://github.com/dev-coop/anny#readme",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "bower": "^1.4.1",
    "chai": "^3.0.0",
    "codeclimate-test-reporter": "^0.1.0",
    "del": "^1.2.0",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.0.1",
    "gulp-eslint": "^0.15.0",
    "gulp-help": "^1.6.0",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "^0.10.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-remember": "^0.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "gulp-webserver": "^0.9.1",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.17",
    "jsdoc": "jsdoc3/jsdoc",
    "lodash": "^3.10.0",
    "mathjs": "^2.1.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "pre-commit": "^1.1.1",
    "require-dir": "^0.3.0",
