{
  "name": "anny",
  "version": "0.1.0",
  "description": "An artificial neural network, yo!",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover _mocha",
    "predocs": "rm -rf docs/$npm_package_version",
    "docs": "jsdoc -c conf.json",
    "docs-flatten": "mv docs/anny/$npm_package_version docs && rm -rf docs/anny",
    "docs-clean-fonts": "rm -rf docs/$npm_package_version/fonts",
    "postdocs": "npm run docs-flatten && npm run docs-clean-fonts",
    "test-watch": "istanbul cover _mocha -- -w",
    "postinstall": "bower install",
    "preinstall": "npm i -g gulp bower",
    "prestart": "npm i",
    "start": "gulp",
    "check-coverage": "istanbul check-coverage",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "engines": {
    "node": "0.12.7",
    "npm": "2.13.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/levithomason/anny.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/levithomason/anny/issues"
  },
  "homepage": "https://github.com/levithomason/anny#readme",
  "dependencies": {},
  "devDependencies": {
    "bower": "^1.4.1",
    "chai": "^3.0.0",
    "codeclimate-test-reporter": "^0.1.0",
    "del": "^1.2.0",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cached": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.0.1",
    "gulp-eslint": "^0.15.0",
    "gulp-help": "^1.6.0",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "^0.10.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-remember": "^0.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "gulp-webserver": "^0.9.1",
    "istanbul": "^0.3.17",
    "jsdoc": "^3.3.2",
    "less": "^2.5.3",
    "lodash": "^3.10.0",
    "mathjs": "^2.1.1",
    "minami": "^1.1.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.1",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.10.1"
  }
}
